{
    "manifest_version": 3,
    "name": "QA HTML",
    "version": "1.0",
    "description": "HTML",
    "action": {
        "default_title": "html",
         "default_icon": "icon.png",
         "default_popup": "popup.html"
      },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
      ],
    "background": {
        "service_worker": "background.js"
    }}